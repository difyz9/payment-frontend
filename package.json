{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:vercel": "NODE_ENV=production VERCEL=1 next build",
    "export": "next export",
    "build:prod": "node scripts/build-prod.js",
    "build:ci": "NODE_ENV=production NEXT_TELEMETRY_DISABLED=1 next build",
    "build:dev": "NODE_ENV=development next build",
    "build:nodejs": "NODE_ENV=production next build",
    "validate:env": "node scripts/validate-env.js",
    "validate:vercel": "node scripts/validate-vercel.js",
    "validate:prod": "node scripts/validate-env.js production",
    "validate:dev": "node scripts/validate-env.js development",
    "prebuild:prod": "npm run validate:prod",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "start:standalone": "cd .next/standalone && NODE_ENV=production PORT=3000 node server.js",
    "test:nodejs": "./test-nodejs-server.sh",
    "deploy:nodejs": "./deploy-nodejs.sh",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next dist out test-nodejs-server",
    "build:clean": "npm run clean && npm run build:nodejs",
    "test:build": "npm run build:nodejs && npm run test:nodejs"
  },
  "dependencies": {
